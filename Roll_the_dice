let x;
let y;
let z;
document.getElementById("button1").onclick=function(){
x=Math.floor(Math.random()*6)+1;
y=Math.floor(Math.random()*6)+1;
z=Math.floor(Math.random()*6)+1;

x=document.getElementById("dice1").innerHTML=x;
y=document.getElementById("dice2").innerHTML=y;
z=document.getElementById("dice3").innerHTML=z;
}
